}

listen {
        ipaddr = *
        port = 0
        type = acct
        limit {
        }
}

authorize {
  preprocess
  rewrite_calling_station_id

  if (!EAP-Message) {
    authorized_macs

    if (!ok) {
      reject
    }
    else {
      update control {
        Auth-Type := Accept
      }
    }
  } else {
    eap
  }
}

